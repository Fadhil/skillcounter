


 <div class="site-wrapper-sign-in">
        <div class="container">
           
            <%= form_for @vet, url: vets_path, method: "post" do |f| %>
            <div class="col-md-4 col-md-offset-4 col-xs-12">
            <form class="form-signin" role="form">
                <h4 class="form-signin-heading">Claim your profile</h4>
                <p>...</p>
                <br>
               
                <%= f.email_field :email, class: "form-control input-lg", placeholder:"Email" %>
               
                 <%= f.text_field :ic_number, class: "form-control input-lg", placeholder:"IC" %>
                
                 <%= f.text_field :licence_number,class: "form-control input-lg", placeholder:"License" %>
                
                 <%= f.submit "Claim Profile",  class:"btn btn-lg btn-primary btn-block" %>
            </form>
            <% end %>
            </div>
        </div>
  </div>
  

    
